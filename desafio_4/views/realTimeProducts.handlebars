<div>
    <h1>Carga de producto</h1>
    <form onsubmit='return addProduct()'>
        <input type="text" name="title" id="title" placeholder="Titulo">
        <input type="text" name="description" id="description" placeholder="Descripcion">
        <input type="text" name="price" id="price" placeholder="Precio">
        <input type="text" name="thumbnail" id="thumbnail" placeholder="Imagen">
        <input type="text" name="code" id="code" placeholder="Codigo">
        <input type="text" name="stock" id="stock" placeholder="Stock">
        <input type="text" name="category" id="category" placeholder="Categoria">
        <input type="text" name="status" id="status" placeholder="Estado">
        <input type="submit" value="Enviar">
    </form>
</div>
<div class="listaProductos" id="listaProductos">
    {{#each products }}
        <div>
            <p>Id: {{this.id}}</p>
            <h2>Titulo: {{this.title}}</h2> 
            <p>Descripcion: {{this.description}}</p> 
            <p>Precio: {{this.price}} </p> 
            <p>Imagenes: {{this.thumbnail}}</p>
            <p>Codigo: {{this.code}}</p> 
            <p>Stock: {{this.stock}}</p> 
            <p>Estado: {{this.status}}</p> 
            <p>Categoria: {{this.category}}</p> 
        </div>
    {{/each}}
</div>
<script></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducst.js"></script>